{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "testRunner": "vitest",
  "reporters": ["clear-text", "progress", "json"],
  "coverageAnalysis": "perTest",
  "mutate": [
    "src/schema-validator.js",
    "src/github.js"
  ],
  "timeoutMS": 60000,
  "tempDirName": ".stryker-tmp",
  "cleanTempDir": true,
  "concurrency": 2,
  "ignoreStatic": true,
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": 50
  }
}
